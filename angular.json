{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bussiness-owners": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {

          "style": "scss",
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/bussiness-owners",
      "sourceRoot": "projects/bussiness-owners/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "baseHref": "/bussinessowners/",
            "outputPath": "dist/bussiness-owners",
            "index": "projects/bussiness-owners/src/index.html",
            "browser": "projects/bussiness-owners/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/bussiness-owners/tsconfig.app.json",
            "assets": [
              "projects/bussiness-owners/src/favicon.ico",
              "projects/bussiness-owners/src/assets"
            ],
            "styles": [
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeflex/primeflex.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "projects/bussiness-owners/src/styles.css"
            ],
            "scripts": [
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "50mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "50mb",
                  "maximumError": "50mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/bussiness-owners/src/environments/environment.ts",
                  "with": "projects/bussiness-owners/src/environments/environment.development.ts"
                }
              ]
            },
            "stage": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/bussiness-owners/src/environments/environment.ts",
                  "with": "projects/bussiness-owners/src/environments/environment.stage.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "bussiness-owners:build:production"
            },
            "development": {
              "buildTarget": "bussiness-owners:build:development"
            },
            "stage": {
              "buildTarget": "bussiness-owners:build:stage"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "bussiness-owners:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/bussiness-owners/tsconfig.spec.json",
            "assets": [
              "projects/bussiness-owners/src/favicon.ico",
              "projects/bussiness-owners/src/assets"
            ],
            "styles": [
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
             "node_modules/primeng/resources/primeng.min.css",
             "/node_modules/primeflex/primeflex.css",
             "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
             "projects/bussiness-owners/src/styles.css"

             
            ],
            "scripts": []
          }
        }
      }
    },
