{
  "name": "microtecadminfrontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:bussiness-owners": "ng build --project bussiness-owners",
    "build:adminportal": "ng build --project adminportal",
    "start:bussiness-owners": "ng serve --project bussiness-owners --port 4301",
    "start:bussiness-owners-stage": "ng serve --project bussiness-owners --port 4301 --configuration stage",
    "start:adminportal": "ng serve --project adminportal --port 4302",
    "start:gateway": "node proxy.js",
    "start:all": "concurrently \"npm run start:gateway\" \"npm run start:bussiness-owners\" \"npm run start:adminportal\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.1.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.1.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "angular-auth-oidc-client": "^16.0.1",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "concurrently": "^8.2.2",
    "ngx-cookie": "^6.0.1",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.10.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular/cli": "^17.0.10",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/jquery": "^3.5.29",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.1.0",
    "typescript": "~5.2.2"
  }
